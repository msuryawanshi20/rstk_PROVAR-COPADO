              <arguments>
                <argument id="soqlQuery">
                  <value class="compound">
                    <parts>
                      <value valueClass="string">select Id,rstk__socntl_avalara__c,rstk__socntl_avatax__c,rstk__socntl_avataxauto__c from rstk__socntl__c where rstk__socntl_div__c='</value>
